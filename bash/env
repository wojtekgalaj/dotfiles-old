
export PATH=$PATH:/usr/local/bin
export PATH=$PATH:~/bin
export PATH=$PATH:/usr/local/sbin
export PATH=$PATH:/usr/local/mysql/bin
export PATH=$PATH:/usr/local/git/bin
export PATH=$PATH:./node_modules/.bin
export TRANSIFEX_USER=helpdesk@packlink.es
export TRANSIFEX_PASSWORD=Cambiala.\!2014
export TRANSIFEX_PROJECT_EBAY=packlinkpro-ebay
export TRANSIFEX_PROJECT_PRO=packlink-pro
export TRANSIFEX_PROJECT_PRO_PUBLIC=packlink-pro-public

export CDPATH="~/bin:~/dev:~/dotvim"

export EDITOR='vim'

source ~/bin/git-completion.bash

NPM_PACKAGES="${HOME}/.npm-packages"

PATH="$NPM_PACKAGES/bin:$PATH"

# Unset manpath so we can inherit from /etc/manpath via the `manpath` command
unset MANPATH # delete if you already modified MANPATH elsewhere in your config
export MANPATH="$NPM_PACKAGES/share/man:$(manpath)"

if [ -n "$BASH_VERSION" ]; then
  source ~/bin/git-completion.bash
fi
