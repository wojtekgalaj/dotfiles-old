alias la='ls -ltar'
alias lh='ls -ltarph'
alias lg='la | grep '

alias peg='ps -ef | grep -v grep | grep'

alias cc='clear'
alias xx='exit'
alias v='nvim'

alias ggu='git pull --rebase upstream'
alias diff='git difftool'
alias merge='git mergetool'
alias ph='git ph'
alias gs='git stash'
alias gsp='git stash pop'
alias s='git status'
alias gco='git checkout'
alias gc='git commit'

alias ss='npm start'
alias sss='gulp serve'
alias tt='npm run test'

alias server="python -m SimpleHTTPServer"

alias gulp="nocorrect gulp"
alias jspm='nocorrect jsmp'

alias vimrc='vim ~/.vimrc'

alias nxconf="cd /usr/local/etc/nginx"
alias cleanup='branch --merged | grep -v "\*" | xargs -n 1 git branch -d'

alias myip="ifconfig | grep \"inet \" | grep -Fv 127.0.0.1 | awk '{print $2}'"

